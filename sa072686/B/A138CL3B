#include<iostream>
using namespace std;
int main()
{
    int a,i,j,k=0;
    bool check;
    char b[1001],c[1001],d[1001];
    while(1)
    {
        cin>>a;
        if(cin.fail()||a==0)break;
        k++;
        for(i=0;i<a;i++)cin>>b[i];
        cout<<"Game "<<k<<":\n";
        while(1)
        {
            for(i=0;i<a;i++)d[i]=0;
            check=1;
            for(i=0;i<a;i++)
            {
                cin>>c[i];
                if(c[i]!=48)check=0;
            }
            if(check==1)break;
            for(i=0;i<a;i++)
            {
                if(b[i]==c[i]){d[i]=1;}
                else for(j=0;j<a;j++)
                {
                    if(b[i]==c[j]&&d[j]==0)
                    {
                        d[j]=2;break;
                    }
                }
            }
            int ka=0,kb=0;
            for(i=0;i<a;i++)
            {
                if(d[i]==1)ka++;
                else if(d[i]==2)kb++;
            }
            cout<<"    ("<<ka<<","<<kb<<")\n";
        }
    }
    return 0;
}



