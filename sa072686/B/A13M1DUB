    <program> ::= <statement>.
    <statement> ::= <command> | <ifstat>
    <ifstat> ::= IF <condition> THEN <statement> ELSE <statement>
    <condition> ::= <cond> | <cond> <op> <condition>
    <op> ::= AND | OR
    <cond> ::= <memory> {= | #} {<command> | NULL}
    <memory> ::= {MY | YOUR} LAST {1 | 2}
    <command> ::= TRADE | CHEAT


