#include<iostream>
#include<math.h>
using namespace std;
bool p[100000]={0};
int k[10000]={0};
int i,j,t=0;

int main()
{
    for(i=2;i<=1000;i++){if(p[i]==0)for(j=i*i;j<100000;j+=i)p[j]=1;}
    for(i=2;i<100000;i++)if(p[i]==0){k[t]=i;t++;}
    while(1)
    {
        long/* long*/ a,su=1;
        cin>>a;
        if(a==1){cout<<"0\n";continue;}
        if(a==0)break;
        j=0;
        for(i=0;k[i]<=sqrt(a)&&i<t;i++)
            if(a%k[i]==0)
            {
                a/=k[i];
                while(a%k[i]==0)
                {
                    a/=k[i];
                    su*=k[i];
                 }
                su*=k[i]-1;
             }
        su*=a-1;
        cout<<su<<"\n";
    }
    return 0;
}

