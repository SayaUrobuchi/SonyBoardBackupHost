        °}¦C¡Gluckynum[] ¡Bans[](©ñ¨«¹LªºÂI)¡Bcheck[](¼Ð°OÂI¥Î)


4. ½Ð¸ÕµÛ«ö·Ó¤W­±ªº¸ê®Æ¡A¼¶¼gµ{¦¡½X¡C

#include<stdio.h>
int k;
int lunum[15], ans[15], check[15];

void visit(int n, int len)
{
    int i;
    if(len >= 6)
    {
        printf("%d", ans[0]);
        for(i=1; i<6; i++)
            printf(" %d", ans[i]);
        printf("\n");
    }
    else
    {
        for(i=n+1; i<k; i++)
        {
            if(check[i] == 1)
            {
                ans[len] = lunum[i];
                check[i] = 0;
                visit(i, len+1);
                check[i] = 1;
            }
        }
    }
}

int main()
{
    int i, cnt = 1;
    while(1)
    {
        scanf("%d", &k);
        if(k == 0) break;
        if((cnt++) != 1) printf("\n");
        for(i=0; i<k; i++)
        {
            scanf("%d", &lunum[i]);
            check[i] = 1;
        }
        visit(-1, 0);


    }
    getchar();
    return 0;
}



