#include <stdio.h>

int ab();

int sum(int a, int b)
{
    return ab(a) + ab(b);
}

int ab(int a)
{
    if(a < 0)
    {
        return -a;
    }
    return a;
}

int main()
{
    printf("%d\n", sum(-5, 20));
    return 0;
}

