#include<stdio.h>
int main()
{
    int a,b,c,d,e,f,g,h,i,j;
    char k[9][9],l[10],m[81][2];
    g=0;
    while(scanf("%d",&k[0][0])==1)
    {
        for(a=0,b=1,j=0;a<9;a++)
        {
            for(b;b<9;b++)
            {
                scanf("%d",&k[a][b]);
                if(!k[a][b])
                {
                    m[j][0]=a;
                    m[j++][1]=b;
                }
            }
            b=0;
        }
        for(a=0;a<9;a++)
        {
            for(b=1;b<10;b++)
            {
                l[b]=0;
            }
            for(b=0;b<9;b++)
            {
                if(k[a][b])
                {
                    if(l[k[a][b]])
                    {
                        break;
                    }
                    l[k[a][b]]=1;
                }
            }
            if(b<9)
            {
                break;
            }
        }
        if(a<9)
        {
            printf("Case %d: Illegal.\n",++g);
            continue;
        }
        for(a=0;a<9;a++)
        {
            for(b=1;b<10;b++)
            {
                l[b]=0;
            }
            for(b=0;b<9;b++)
            {
                if(k[b][a])
                {
                    if(l[k[b][a]])
                    {
                        break;
                    }
                    l[k[b][a]]=1;
                }
            }
            if(b<9)
            {
                break;
            }
        }
        if(a<9)
        {
            printf("Case %d: Illegal.\n",++g);
            continue;
        }
        for(a=0;a<9;a+=3)
        {
            for(b=0;b<9;b+=3)
            {
                for(c=1;c<10;c++)
                {
                    l[c]=0;
                }
                for(c=a,d=a+3,f=b+3;c<d;c++)
                {
                    for(e=b;e<f;e++)
                    {
                        if(k[c][e])
                        {
                            if(l[c][e])
                            {
                                break;
                            }
                            l[c][e]=1;
                        }
                    }
                    if(e<f)
                    {
                        break;
                    }
                }
                if(c<d)
                {
                    break;
                }
            }
            if(b<9)
            {
                break;
            }
        }
        if(a<9)
        {
            printf("Case %d: Illegal.\n",++g);
            continue;
        }
        h=1;
        while(1)
        {
            for(c=0;c<j;c++)
            {
                a=m[c][0];
                b=m[c][1];
                for(d=1;d<10;d++)
                {
                    l[d]=1;
                }
                for(d=0;d<a;d++)
                {
                    if(k[a][d])
                    {
                        l[k[a][d]]=0;
                    }
                }
                for(d=0;d<a;d++)
                {
                    if(k[d][b])
                    {
                        l[k[d][b]]=0;
                    }
                }
            }
        }
    }
    return 0;
}



