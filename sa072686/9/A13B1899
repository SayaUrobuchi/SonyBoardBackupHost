#include<iostream>
#include<string.h>
using namespace std;
int i,d=0,x[123]={0};
char t[2001]={0},g[2001]={0},no[2001]={0};
bool f[2001];
void dfs(int k)
{
    int j;
    for(j=0;t[j]!='\0';j++)
    {
        if(f[j]==0)//ÀË¬d¨«¹L¨S
        {
            g[k]=t[j];
            f[j]=1;
            dfs(k+1);
            f[j]=0;
        }
    }
    if(k==d)//compare(¿ù¤F)
    {
        for(j=k;j>=0;j--)
            if(g[j]!=no[j])
            {
                for(j=0;j<=k;j++)
                {
                    cout<<g[j];
                    no[j]=g[j];
                }
                cout<<"\n";
                break;
            }
    }
}
int main()
{
    for(i='A';i<='Z';i++)//«Øªí
    {
        x[i]=d;
        x[i+32]=d+1;
        d+=2;
    }
    int n;
    int j;
    cin>>n;
    cin.get();
    while(n)
    {
        cin.getline(t,2000);
        d=strlen(t)-1;
        for(i=1;i<=d;i++)
            for(j=i;j>=0;j--)
            {
                if(x[t[j]]<x[t[j-1]])t[j]^=t[j-1]^=t[j]^=t[j-1];
                else break;
            }//bubble sort
        for(i=0;i<2001;i++){f[i]=0;g[i]=0;}
        dfs(0);
        n--;
    }
    return 0;
}





