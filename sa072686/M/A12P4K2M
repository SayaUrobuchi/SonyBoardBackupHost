¹ê§@½d¨Ò¡G

n=10;
l=1;
r=5;
while(1)
{
    c=(l+r)/2;
    if(ary[c]==n)
    {
        break;
    }
    else if(ary[c]>n)
    {
        r=c-1;
        if(l>r)
        {
            break;
        }
    }
    else
    {
        l=c+1;
        if(l>r)
        {
            break;
        }
    }
}

if(ary[c]==n)
{
    printf("%d is located in ary[%d].\n",n,c);
}
else
{
    printf("Cannot find %d in ary.\n",n);
}



