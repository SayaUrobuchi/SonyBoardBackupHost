typedef struct abc
{
    int a;
    char b[10];
}abc;

abc k[100];

int abcc(const void *x,const void *y)
{
    if((*(abc*)x).a<(*(abc*)y).a)
    {
        return -1;
    }
    else if((*(abc*)x).a==(*(abc*)y).a)
    {
        int p;
        for(p=0;p<strlen((*(abc*)x).z)&&p<strlen(*(abc*)y).z;p++)
        {
            if((*(abc*)x).z[p]<(*(abc*)y).z[p])
            {
                return -1;
            }
            else if((*(abc*)x).z[p]>(*(abc*)y).z[p])
            {
                return 1;
            }
        }
        return 0;
    }
    else
    {
        return 1;
    }
}

qsort(k,100,sizeof(abc),abcc);

