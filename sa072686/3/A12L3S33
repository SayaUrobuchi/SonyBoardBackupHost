int abcc(int p)
{
    int w,x;
    if(p==3)
    {
        return 1;
    }
    else if(p>3)
    {
        return 0;
    }
    w=abcc(p+1);
    x=abcc(p+2);
    return w+x;
}

int main()
{
    printf("%d\n",abcc(0));
    return 0;
}

