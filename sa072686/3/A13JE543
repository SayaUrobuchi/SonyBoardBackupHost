#include<stdio.h>
#include<math.h>
int t,n,i,j,an,c,b[10][5];
double a[10];
main(){
    while(scanf("%d",&n)&&n!=-1){
        t++;
        for(i=c=0;i<n;i++){
            scanf("%lf",&a[i]);
            for(j=0;j<3;j++)scanf("%d",&b[i][j]);
            b[i][3]=b[i][1]+b[i][2]+b[i][0];
            b[i][4]=b[i][0]+b[i][1];
        }
        printf("Case %d: ",t);
        if(n==0){
            printf("30-60\n");
            continue;
        }
        for(i=30,an=0;i<=61;i++){
            for(j=0;i!=61&&j<n;j++){

if(a[j]*3600/i-floor((a[j]*3600/i/b[j][3]))*b[j][3]>b[j][4])break;
            }
            if(j<n){
                if(an!=0){
                    if(c!=0)printf(", ");
                    if(i-1==an)printf("%d",an);
                    else printf("%d-%d",an,i-1);
                    c=1;
                    an=0;
                }
            }
            else{
                if(an==0)an=i;
            }
        }
        if(c==0)puts("No acceptable speeds.");
        else puts("");
    }
}


