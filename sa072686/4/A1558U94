[m#include<iostream>
[m#include<algorithm>     /* ¤Þ¤J sort() */
[m#include<ctime>         /* ¤Þ¤J clock(), time(), rand() */
[m#define N 4000000
[musing namespace std;
[m
[m    int n,s[N];         /* n: ±ý±Æ§Çªº­Ó¼Æ */
[m    clock_t start,stop; /* ½Xªíªº¶}©lÂI»Pµ²§ôÂI */
[m
[mint main()
[m{
[m    srand(  time( NULL )  );    /* ³]©w rand() ¥Îªº¶Ã¼ÆºØ¤l */
[m
[m    while( cin>>n )
[m    {
[m        for( int i=0;i<n;i++ )  /* ¼g¤J n ­Ó¶Ã¼Æ */
[m            s[i] = rand()%N;
[m
[m        start = clock();        /* ­p®É¶}©l */
[m        sort( s,s+n );          /* ±Æ§Ç¡A§Y±ý­p®Éªºµ{§Ç */
[m        stop = clock();         /* ­p®Éµ²§ô */
[m
[m        cout<<"Sorting "<<n<<" element(s) needs "
[m            <<(  stop-start  ) <<" millisecond(s)."<<endl;
[m
[m        /* £Gt = stop-start (ms) */
[m    }
[m}
