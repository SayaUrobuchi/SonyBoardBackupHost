#include <stdio.h>
#include <stdlib.h>

int main()
{
    char str[1001];
    FILE *file;
    while(gets(str))  /* Åª¤J¥|«h¹Bºâ¦¡ */
    {
        file = fopen("C:\123.c", "w");  /* ¶}±Ò¤@­ì©l½XÀÉ®× */
        fprintf(file, "#include <stdio.h>\n\n");  /* ¼g¤Jµ{¦¡½X */
        fprintf(file, "int main() {\n");       /* ¨Ï¨ä¿é¥X¸Ó¥|«h¹Bºâ¦¡ */
