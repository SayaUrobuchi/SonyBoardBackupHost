#include<iostream>
using namespace std;
int t[1002][1002]={0};
main()
{
    int n;
    while(cin>>n)
    {
        int DF[1001]={0},BF[1001]={0},i,DF_layer[1001],layer=0,j,k;
        for(i=0;i<n;i++)cin>>BF[i];
        for(i=0;i<n;i++)cin>>DF[i];
        for(i=0;i<=n;i++){t[i][1]=0;DF_layer[i]=-1;t[i][0]=0;}//²MªÅ
        t[BF[0]][1]=BF[1];
        DF_layer[0]=0;
        layer++;
        t[BF[0]][2]=0;
        t[BF[0]][0]++;
        DF_layer[1]=layer;
        for(i=2;i<n;i++)
        {
            for(j=0;j<n;j++)
            {
                if(BF[i-1]==DF[j])
                {
                    bool f=0;
                    for(k=j+1;k<n;k++)
                    {
                        if(BF[i]==DF[k])
                        {
                            for(int x=k-1;x>=0;x--)
                            {
                                if(DF_layer[x]==layer-1)
                                {
                                    DF_layer[k]=layer;
                                    t[DF[x]][t[DF[x]][0]+1]=BF[i];
                                    t[DF[x]][t[DF[x]][0]+2]=0;//²MªÅ
                                    t[DF[x]][0]++;
                                    break;
                                }
                            }
                            f=1;break;
                        }
                    }
                    if(f==0)
                    {
                        layer++;
                        for(k=0;k<j;k++)if(DF[k]==BF[i])break;
                        for(int x=k;x>=0;x--)
                        {
                            if(DF_layer[x]==layer-1)
                            {
                                DF_layer[k]=layer;
                                t[DF[x]][t[DF[x]][0]+1]=BF[i];
                                t[DF[x]][t[DF[x]][0]+2]=0;//²MªÅ
                                t[DF[x]][0]++;
                                break;
                            }
                        }
                    }
                }
            }
        }
        for(i=1;i<=n;i++)
        {

for(j=1;t[i][j]>0;j++)for(k=2;t[i][k]>0;k++)if(t[i][k]<t[i][k-1]){int
a=t[i][k];t[i][k]=t[i][k-1];t[i][k-1]=a;};
        }//bubble sort
        for(i=1;i<=n;i++)
        {
            cout<<i<<":";
            for(j=1;t[i][j]>0;j++)cout<<" "<<t[i][j];
            cout<<"\n";
        }//¿é¥X
    }
}


--
